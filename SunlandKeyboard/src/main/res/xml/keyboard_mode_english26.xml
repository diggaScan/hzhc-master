<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:horizontalGap="0px"
    android:keyHeight="10%p"
    android:keyWidth="10%p"
    android:verticalGap="0px">

    <Row>
        <Key
            android:codes="@integer/KEYBORD_NUMBER_1"
            android:keyEdgeFlags="left"
            android:keyLabel="1" />
        <Key
            android:codes="@integer/KEYBORD_NUMBER_2"
            android:keyLabel="2" />
        <Key
            android:codes="@integer/KEYBORD_NUMBER_3"
            android:keyLabel="3" />
        <Key
            android:codes="@integer/KEYBORD_NUMBER_4"
            android:keyLabel="4" />
        <Key
            android:codes="@integer/KEYBORD_NUMBER_5"
            android:keyLabel="5" />
        <Key
            android:codes="@integer/KEYBORD_NUMBER_6"
            android:keyLabel="6" />
        <Key
            android:codes="@integer/KEYBORD_NUMBER_7"
            android:keyLabel="7" />
        <Key
            android:codes="@integer/KEYBORD_NUMBER_8"
            android:keyLabel="8" />
        <Key
            android:codes="@integer/KEYBORD_NUMBER_9"
            android:keyLabel="9" />
        <Key
            android:codes="@integer/KEYBORD_NUMBER_0"
            android:keyEdgeFlags="right"
            android:keyLabel="0" />
    </Row>

    <Row>
        <Key
            android:codes="@integer/KEYBORD_ENGLISH_q"
            android:keyEdgeFlags="left"
            android:keyLabel="q" />
        <Key
            android:codes="@integer/KEYBORD_ENGLISH_w"
            android:keyLabel="w" />
        <Key
            android:codes="@integer/KEYBORD_ENGLISH_e"
            android:keyLabel="e" />
        <Key
            android:codes="@integer/KEYBORD_ENGLISH_r"
            android:keyLabel="r" />
        <Key
            android:codes="@integer/KEYBORD_ENGLISH_t"
            android:keyLabel="t" />
        <Key
            android:codes="@integer/KEYBORD_ENGLISH_y"
            android:keyLabel="y" />
        <Key
            android:codes="@integer/KEYBORD_ENGLISH_u"
            android:keyLabel="u" />
        <Key
            android:codes="@integer/KEYBORD_ENGLISH_i"
            android:keyLabel="i" />
        <Key
            android:codes="@integer/KEYBORD_ENGLISH_o"
            android:keyLabel="o" />
        <Key
            android:codes="@integer/KEYBORD_ENGLISH_p"
            android:keyEdgeFlags="right"
            android:keyLabel="p" />
    </Row>

    <Row>
        <Key
            android:codes="@integer/KEYBORD_ENGLISH_a"
            android:horizontalGap="5%p"
            android:keyEdgeFlags="left"
            android:keyLabel="a" />
        <Key
            android:codes="@integer/KEYBORD_ENGLISH_s"
            android:keyLabel="s" />
        <Key
            android:codes="@integer/KEYBORD_ENGLISH_d"
            android:keyLabel="d" />
        <Key
            android:codes="@integer/KEYBORD_ENGLISH_f"
            android:keyLabel="f" />
        <Key
            android:codes="@integer/KEYBORD_ENGLISH_g"
            android:keyLabel="g" />
        <Key
            android:codes="@integer/KEYBORD_ENGLISH_h"
            android:keyLabel="h" />
        <Key
            android:codes="@integer/KEYBORD_ENGLISH_j"
            android:keyLabel="j" />
        <Key
            android:codes="@integer/KEYBORD_ENGLISH_k"
            android:keyLabel="k" />
        <Key
            android:codes="@integer/KEYBORD_ENGLISH_l"
            android:keyEdgeFlags="right"
            android:keyLabel="l" />
    </Row>
    <Row>
        <Key
            android:codes="@integer/KEYBORD_SHIFT"
            android:isModifier="true"
            android:isSticky="false"
            android:keyEdgeFlags="left"
            android:keyLabel="大写"
            android:keyWidth="15%p" />
        <Key
            android:codes="@integer/KEYBORD_ENGLISH_z"
            android:keyLabel="z" />
        <Key
            android:codes="@integer/KEYBORD_ENGLISH_x"
            android:keyLabel="x" />
        <Key
            android:codes="@integer/KEYBORD_ENGLISH_c"
            android:keyLabel="c" />
        <Key
            android:codes="@integer/KEYBORD_ENGLISH_v"
            android:keyLabel="v" />
        <Key
            android:codes="@integer/KEYBORD_ENGLISH_b"
            android:keyLabel="b" />
        <Key
            android:codes="@integer/KEYBORD_ENGLISH_n"
            android:keyLabel="n" />
        <Key
            android:codes="@integer/KEYBORD_ENGLISH_m"
            android:keyLabel="m" />
        <Key
            android:codes="@integer/KEYBORD_DEL"
            android:isRepeatable="true"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/del"
            android:keyWidth="15%p" />
    </Row>
    <Row android:rowEdgeFlags="bottom">
        <Key
            android:codes="@integer/KEYBORD_CHINESE"
            android:keyEdgeFlags="left"
            android:keyLabel="中文"
            android:keyWidth="20%p" />
        <Key
            android:codes="@integer/KEYBORD_NUM"
            android:keyLabel="123"
            android:keyWidth="15%p" />
        <Key
            android:codes="@integer/KEYBORD_BLANK"
            android:isRepeatable="true"
            android:keyIcon="@drawable/backspace"
            android:keyWidth="30%p" />
        <Key
            android:codes="@integer/KEYBORD_SYMBOLS"
            android:keyLabel="符号"
            android:keyWidth="15%p" />

        <Key
            android:codes="@integer/KEYBORD_DONE"
            android:keyEdgeFlags="right"
            android:keyLabel="完成"
            android:keyWidth="20%p" />
    </Row>

</Keyboard>