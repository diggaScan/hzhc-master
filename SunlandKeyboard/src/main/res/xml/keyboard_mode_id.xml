<?xml version="1.0" encoding="utf-8"?>
<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:horizontalGap="0px"
    android:keyHeight="10%p"
    android:keyWidth="33%p"
    android:verticalGap="0px">

    <Row>
        <Key android:codes="@integer/KEYBORD_NUMBER_1" android:keyLabel="1" android:keyWidth="25%p" />
        <Key android:codes="@integer/KEYBORD_NUMBER_2" android:keyLabel="2" android:keyWidth="25%p"/>
        <Key android:codes="@integer/KEYBORD_NUMBER_3" android:keyLabel="3" android:keyWidth="25%p"/>
        <Key android:codes="@integer/KEYBORD_QUICK1" android:keyLabel="3301" android:keyWidth="25%p"/>
    </Row>
    <Row>
        <Key android:codes="@integer/KEYBORD_NUMBER_4" android:keyLabel="4" android:keyWidth="25%p"/>
        <Key android:codes="@integer/KEYBORD_NUMBER_5" android:keyLabel="5" android:keyWidth="25%p"/>
        <Key android:codes="@integer/KEYBORD_NUMBER_6" android:keyLabel="6" android:keyWidth="25%p"/>
        <Key android:codes="@integer/KEYBORD_QUICK2" android:keyLabel="3302" android:keyWidth="25%p"/>
    </Row>
    <Row>
        <Key android:codes="@integer/KEYBORD_NUMBER_7" android:keyLabel="7" android:keyWidth="25%p"/>
        <Key android:codes="@integer/KEYBORD_NUMBER_8" android:keyLabel="8" android:keyWidth="25%p"/>
        <Key android:codes="@integer/KEYBORD_NUMBER_9" android:keyLabel="9" android:keyWidth="25%p"/>
        <Key android:codes="@integer/KEYBORD_QUICK3" android:keyLabel="3303" android:keyWidth="25%p"/>
    </Row>
    <Row>
        <Key android:codes="@integer/KEYBORD_ENGLISH_X" android:keyLabel="X" android:keyWidth="25%p"/>
        <Key android:codes="@integer/KEYBORD_NUMBER_0" android:keyLabel="0" android:keyWidth="25%p"/>
        <Key android:codes="@integer/KEYBORD_DEL" android:keyIcon="@drawable/del" android:isRepeatable="true" android:keyWidth="25%p"/>
        <Key android:codes="@integer/KEYBORD_DONE" android:keyLabel="完成" android:keyWidth="25%p"/>
    </Row>
</Keyboard>  
