<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="@dimen/info_display_item_height"
                android:gravity="center_vertical"
                android:maxEms="5"
                android:text="姓名" />

            <TextView
                android:id="@+id/name"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/info_display_item_height" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="@dimen/info_display_item_height"
                android:gravity="center_vertical"
                android:maxEms="5"
                android:text="英文姓名" />

            <TextView
                android:id="@+id/eng_name"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/info_display_item_height" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="@dimen/info_display_item_height"
                android:gravity="center_vertical"
                android:maxEms="5"
                android:text="性别" />

            <TextView
                android:id="@+id/gender"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/info_display_item_height" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="@dimen/info_display_item_height"
                android:gravity="center_vertical"
                android:maxEms="5"
                android:text="国籍" />

            <TextView
                android:id="@+id/nation"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/info_display_item_height" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="@dimen/info_display_item_height"
                android:gravity="center_vertical"
                android:maxEms="5"
                android:text="出生日期" />

            <TextView
                android:id="@+id/birth"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/info_display_item_height" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="@dimen/info_display_item_height"
                android:gravity="center_vertical"
                android:maxEms="5"
                android:text="出生地" />

            <TextView
                android:id="@+id/birth_place"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/info_display_item_height" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="@dimen/info_display_item_height"
                android:gravity="center_vertical"
                android:maxEms="5"
                android:text="籍贯" />

            <TextView
                android:id="@+id/origin_place"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/info_display_item_height" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="@dimen/info_display_item_height"
                android:gravity="center_vertical"
                android:maxEms="5"
                android:text="身份证" />

            <TextView
                android:id="@+id/id_num"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/info_display_item_height" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="@dimen/info_display_item_height"
                android:gravity="center_vertical"
                android:maxEms="5"
                android:text="护照" />

            <TextView
                android:id="@+id/passport"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/info_display_item_height" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="@dimen/info_display_item_height"
                android:gravity="center_vertical"
                android:maxEms="5"
                android:text="港澳通行证" />

            <TextView
                android:id="@+id/ga_passport"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/info_display_item_height" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="@dimen/info_display_item_height"
                android:gravity="center_vertical"
                android:maxEms="5"
                android:text="台湾同行证" />

            <TextView
                android:id="@+id/taiwan_passport"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/info_display_item_height" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="@dimen/info_display_item_height"
                android:gravity="center_vertical"
                android:maxEms="5"
                android:text="居住证" />

            <TextView
                android:id="@+id/live_cert"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/info_display_item_height" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="@dimen/info_display_item_height"
                android:gravity="center_vertical"
                android:maxEms="5"
                android:text="民族" />

            <TextView
                android:id="@+id/ethnic_group"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/info_display_item_height" />
        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="@dimen/info_display_item_height"
                android:gravity="center_vertical"
                android:maxEms="5"
                android:text="驾驶证" />

            <TextView
                android:id="@+id/driver_license"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/info_display_item_height" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="@dimen/info_display_item_height"
                android:gravity="center_vertical"
                android:maxEms="5"
                android:text="杭州市民卡" />

            <TextView
                android:id="@+id/citizen_card"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/info_display_item_height" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="@dimen/info_display_item_height"
                android:gravity="center_vertical"
                android:maxEms="5"
                android:text="电话" />

            <TextView
                android:id="@+id/phone_num"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/info_display_item_height" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="@dimen/info_display_item_height"
                android:gravity="center_vertical"
                android:maxEms="5"
                android:text="婚姻登记" />

            <TextView
                android:id="@+id/marriage_reg"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/info_display_item_height" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="@dimen/info_display_item_height"
                android:gravity="center_vertical"
                android:maxEms="5"
                android:text="工作单位" />

            <TextView
                android:id="@+id/gzdw"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/info_display_item_height" />
        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="@dimen/info_display_item_height"
                android:gravity="center_vertical"
                android:maxEms="5"
                android:text="车船登记" />

            <TextView
                android:id="@+id/car_reg"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/info_display_item_height" />
        </LinearLayout>

    </LinearLayout>


</ScrollView>